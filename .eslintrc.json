{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-empty": "off",
        "no-unused-vars": ["error",
            { "vars": "all",
              "args": "none",
              "ignoreRestSiblings": false
            }]
    }
}
